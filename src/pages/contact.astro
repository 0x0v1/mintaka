---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Barghest Asia: Get in Touch",
	description:
		"Contact Barghest Asia for business inquiries and opportunities. Reach us at info@barghest.asia",
	canonical: "https://barghest.asia/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
